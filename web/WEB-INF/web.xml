<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>cl.inacap.cdn.controllers.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProyectServlet</servlet-name>
        <servlet-class>cl.inacap.cdn.controllers.ProyectServlet</servlet-class>    
    </servlet>
    <servlet>
        <servlet-name>YearServlet</servlet-name>
        <servlet-class>cl.inacap.cdn.controllers.YearServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PresupuestoServlet</servlet-name>
        <servlet-class>cl.inacap.cdn.controllers.PresupuestoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GastoServlet</servlet-name>
        <servlet-class>cl.inacap.cdn.controllers.GastoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PdfServlet</servlet-name>
        <servlet-class>cl.inacap.cdn.controllers.PdfServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidarServlet</servlet-name>
        <servlet-class>cl.inacap.cdn.controllers.ValidarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioServlet</servlet-name>
        <servlet-class>cl.inacap.cdn.controllers.UsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TipoUsuarioServlet</servlet-name>
        <servlet-class>cl.inacap.cdn.controllers.TipoUsuarioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/Login.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProyectServlet</servlet-name>
        <url-pattern>/Proyect.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>YearServlet</servlet-name>
        <url-pattern>/Year.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PresupuestoServlet</servlet-name>
        <url-pattern>/Presupuesto.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GastoServlet</servlet-name>
        <url-pattern>/Gasto.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PdfServlet</servlet-name>
        <url-pattern>/PDF</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidarServlet</servlet-name>
        <url-pattern>/Validar.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/Usuario.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TipoUsuarioServlet</servlet-name>
        <url-pattern>/TipoUsu.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
